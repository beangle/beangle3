<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<jmxConfigurator/>
	<!-- 控制台输出日志 -->
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
	<root>
		<level value="INFO"/>
		<appender-ref ref="STDOUT"/>
	</root>
</configuration>
