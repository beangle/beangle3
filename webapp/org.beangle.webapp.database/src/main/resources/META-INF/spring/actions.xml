<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean name="database.sql" scope="prototype"
	 class="org.beangle.webapp.database.action.SqlAction" 
	    autowire="byName">
	</bean>
	
	<bean name="database.connection" scope="prototype"
	 class="org.beangle.webapp.database.action.ConnectionAction" 
	    autowire="byName">
	</bean>
	
	<bean name="database.datasource" scope="prototype"
	 class="org.beangle.webapp.database.action.DatasourceAction" 
	    autowire="byName">
	    <property name="entityName" value="org.beangle.webapp.database.model.DatasourceBean"/>
	</bean>
	
	<bean name="database.provider" scope="prototype"
	 class="org.beangle.webapp.database.action.ProviderAction" 
	    autowire="byName">
	    <property name="entityName" value="org.beangle.webapp.database.model.DatasourceProviderBean"/>
	</bean>
</beans>
